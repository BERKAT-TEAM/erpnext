 "html": "<!DOCTYPE html>\n\n<html class=\"print-format landscape\" >\n<meta name=\"pdfkit-orientation\" content=\"Landscape\">\n<style>\n    @media screen {\n        .print-format-gutter {\n            background-color: #ddd;\n            padding: 15px 0px;\n        }\n        .print-format {\n            background-color: white;\n            box-shadow: 0px 0px 9px rgba(0,0,0,0.5);\n            max-width: 8.3in;\n            min-height: 11.69in;\n            padding: 0.75in;\n            margin: auto;\n        }\n\n        .print-format.landscape {\n            max-width: 11.69in;\n            padding: 0.2in;\n        }\n    }\n\ttable, th {\n        text-align: left;\n\t\tpadding: 0px;\n        font-size: 10px !important;\n\t\tmargin: 0px !important;\n\t}\n\n    td {\n        text-align: left;\n\t\tpadding: 0px;\n        font-size: 9px;\n\t\tmargin: 0px !important;\n        border-right: 1px solid black; \n        border-left: 1px solid black;\n\t}\n\n    table, thead, tr, th.OK{\n    \twidth: 10px;\n    }\n    th{\n        /* height:0px !important; */\n        border-right: 1px solid black; \n        border-left: 1px solid black;\n        border-top: 1px solid black;\n    }\n\n    table,tr{\n        height: 5px;\n        border-right: 1px;\n    }\n\n    .settingI_left{\n        text-align: left;\n    }\n\n    .letter-head-quo{\n        border:none !important;\n        height:1px !important;\n        /* padding: 1px !important; */\n        font-size: 9.5px !important;\n        padding: 5px; \n        margin: 5px;\n        border-spacing: 0;\n        border-collapse: collapse;\n        line-height: 7px;\n        /* display: block; */\n    }\n\n    .header-quo{\n        text-align: center !important;\n        vertical-align: middle !important;\n        font-size: 12px;\n        font-weight: bold;\n        vertical-align : middle !important;\n        text-align:center;\n        background-color:#09b4ac !important;\n        /* color:white; */\n        border-top: 1px solid black;\n    }\n    .footer-quo{\n        border:none !important;\n        text-align: right !important;\n        vertical-align: middle !important;\n        font-size: 9px;\n        font-weight: bold;\n        vertical-align : middle !important;\n        text-align:center;\n        background-color:#09b4ac !important;\n        /* color:white; */\n    }\n\n    .center-cropped {\n        object-fit: none; /* Do not scale the image */\n        object-position: center; /* Center the image within the element */\n        height: 100px;\n        width: 200px;\n    }\n\n    .footer-quo-signed{\n\n    }\n    \n</style>\n<body>\n{% set links = frappe.get_all('Dynamic Link', filters={'link_doctype': 'Company', 'link_name': doc.company, 'parenttype': 'Address'}, fields=['parent']) %}\n{% set links1 = frappe.get_all('Dynamic Link', filters={'link_doctype': 'Company', 'link_name': doc.company}) %}\n{% if links %}\n    {% set address = frappe.get_doc(\"Address\", links[0].parent) %}\n    {% set phone_no = frappe.db.get_value(\"Company\", doc.company, [\"phone_no\"]) %}\n{% endif %} \n{% set UID = frappe.session.user %}\n{% set fullname = frappe.get_fullname(doc.owner)%}\n{% set email_user = frappe.db.get_value(\"User\", doc.owner, [\"email\"]) %}\n{% set phonenum = frappe.db.get_value(\"User\", doc.owner, [\"phone\"]) %}\n{% if frappe.db.get_value(\"User\", UID, [\"sales_signature\"]) %}\n    {% set signature_user = frappe.db.get_value(\"User\", doc.owner, [\"sales_signature\"]) %}\n{% else %}\n    {% set signature_user = frappe.db.get_value(\"User\", doc.owner, [\"signature\"]) %}\n{% endif %}\n\n{% if doc.address_display %}\n    {% set addr_1line = doc.address_display.split(', ')%}\n    {% if addr_1line|length >= 2 %}\n        {% set addr_cust = addr_1line[0] +\", \"+ addr_1line[1] %}\n    {% elif addr_1line|length == 1 %}\n        {% set addr_cust = addr_1line[0] %}\n    {% else %}\n        {% set addr_cust = doc.address_display %}\n    {% endif %}\n{% else %}\n    {% set addr_cust = \"-\" %}\n{% endif %}\n\n{% set percent_disc =  doc.additional_discount_percentage %}\n\n{% set date_now_doc = frappe.utils.formatdate(frappe.utils.today(), 'dd-MM-YYYY') %}\n{% set pajak_reklame = frappe.format_value(doc.pajak_reklame, {'fieldtype': 'Currency', 'options': 'IDR'}, doc) %}\n{% set grand_total = frappe.format_value(doc.grand_total, {'fieldtype': 'Currency', 'options': 'IDR'}, doc) %}\n{% set final_total = doc.grand_total + doc.pajak_reklame %}\n{% set final_total = frappe.format_value(final_total, {'fieldtype': 'Currency', 'options': 'IDR'}, doc) %}\n\n{% set modif_date_data_temp = frappe.utils.formatdate(frappe.utils.today(), 'dd-MM-YYYY').split('-') %}\n\n{% if modif_date_data_temp[1] == \"1\" or modif_date_data_temp[1] == \"01\" %}\n    {% set month_date_modif = \"January\" %}\n{% elif modif_date_data_temp[1] == \"2\" or modif_date_data_temp[1] == \"02\" %}\n    {% set month_date_modif = \"February\" %}\n{% elif modif_date_data_temp[1] == \"3\" or modif_date_data_temp[1] == \"03\" %}\n    {% set month_date_modif = \"March\" %}\n{% elif modif_date_data_temp[1] == \"4\" or modif_date_data_temp[1] == \"04\" %}\n    {% set month_date_modif = \"April\" %}\n{% elif modif_date_data_temp[1] == \"5\" or modif_date_data_temp[1] == \"05\" %}\n    {% set month_date_modif = \"May\" %}\n{% elif modif_date_data_temp[1] == \"6\" or modif_date_data_temp[1] == \"06\" %}\n    {% set month_date_modif = \"June\" %}\n{% elif modif_date_data_temp[1] == \"7\" or modif_date_data_temp[1] == \"07\" %}\n    {% set month_date_modif = \"July\" %}\n{% elif modif_date_data_temp[1] == \"8\" or modif_date_data_temp[1] == \"08\" %}\n    {% set month_date_modif = \"August\" %}\n{% elif modif_date_data_temp[1] == \"9\" or modif_date_data_temp[1] == \"09\" %}\n    {% set month_date_modif = \"September\" %}\n{% elif modif_date_data_temp[1] == \"10\" or modif_date_data_temp[1] == \"10\" %}\n    {% set month_date_modif = \"October\" %}\n{% elif modif_date_data_temp[1] == \"11\" or modif_date_data_temp[1] == \"11\" %}\n    {% set month_date_modif = \"November\" %}\n{% elif modif_date_data_temp[1] == \"12\" or modif_date_data_temp[1] == \"12\" %}\n    {% set month_date_modif = \"December\" %}\n{% else %}\n    {% set month_date_modif = \"FAIL GET MONTH\" %}\n{% endif %}\n\n{% if modif_date_data_temp[0][0] == \"0\" %}\n    {% set modif_date_data = modif_date_data_temp[0][1] + \" \" + month_date_modif + \" \" + modif_date_data_temp[2] %}\n{% else %}\n    {% set modif_date_data = modif_date_data_temp[0] + \" \" + month_date_modif + \" \" + modif_date_data_temp[2] %}\n{%endif%}\n\n{% set TOC = doc.terms %}\n\n{% if frappe.db.get_value(\"Contact\", doc.contact_display, [\"salutation\"]) %}\n    {% set customer_salutation = frappe.db.get_value(\"Contact\", doc.contact_display, [\"salutation\"]) %}\n{% else %}\n    {% set customer_salutation = \"\" %}\n{% endif %}\n\n{% if frappe.db.get_value(\"Contact\", doc.contact_display, [\"designation\"]) %}\n    {% set customer_designation = frappe.db.get_value(\"Contact\", doc.contact_display, [\"designation\"]) %}\n{% else %}\n    {% set customer_designation = \"\" %}\n{% endif %}\n\n\n<table style=\"table-layout: fixed; border: 1px !important\">\n    <colgroup>\n        <col span=\"1\" style=\"width: 30px;\">\n        <col span=\"1\" style=\"width: 80px;\">\n        <col span=\"1\" style=\"width: 70px;\">\n        <col span=\"1\" style=\"width: 80px;\">\n        <col span=\"1\" style=\"width: 100px;\">\n        <col span=\"1\" style=\"width: 100px;\">\n        <col span=\"1\" style=\"width: 80px;\">\n        <col span=\"1\" style=\"width: 105px;\">\n        <col span=\"1\" style=\"width: 30px;\">\n        <col span=\"1\" style=\"width: 80px;\">\n        <col span=\"1\" style=\"width: 180px;\">\n     </colgroup>\n    <thead style=\"border:1px !important;\">\n        <tr class=\"letter-head-quo\">\n            <!-- <th rowspan=\"2\" style=\"vertical-align : middle;text-align:center; border-right:1px solid black;\">No.</th> -->\n            <th class=\"letter-head-quo\" colspan=\"3\" style=\"vertical-align : middle;text-align:left !important; border-right:1px solid black; width:200px;\">\n                <img src=\"/files/ming_blue.png\" alt=\"\" height=50px width=150px>\n            </th>\n            <th class=\"letter-head-quo\" style=\"vertical-align : top;text-align:left !important; border-right:1px solid black;\">\n                <img src=\"/files/ming_iso.png\" alt=\"\" height=48px width=48px>\n            </th>\n            <th class=\"letter-head-quo\" colspan=\"4\"></th>\n            <th class=\"letter-head-quo\" colspan=\"3\" style=\"text-align: left !important; vertical-align: top !important; font-size: 14px !important; line-height: 14px !important;\">\n                Jakarta, {{ modif_date_data }}\n            </th>\n        </tr>\n        <tr class=\"letter-head-quo\">\n            <th class=\"letter-head-quo\" colspan=\"5\" style=\"vertical-align : middle !important;text-align:center; border-right:1px solid black; border-bottom:1px solid black;\">\n            </th>\n            <th class=\"letter-head-quo\" colspan=\"2\" rowspan=\"2\" style=\"vertical-align : bottom !important;text-align:center; border-right:1px solid black; font-size: 18px !important; border-bottom:1px solid black; line-height: 12px !important;\">\n                <u>Quotation</u>\n            </th>\n            <th class=\"letter-head-quo\" rowspan=\"2\"></th>\n            <th class=\"letter-head-quo\" colspan=\"3\" style=\"font-size: 11px !important; vertical-align : middle !important;text-align:left; border-right:1px solid black; border-bottom:1px solid black;\">\n                PT. MITRA INOVASI GEMILANG\n            </th>\n        </tr>\n        <tr class=\"letter-head-quo\">\n            <th colspan=\"4\" class=\"letter-head-quo\">\n                Customer Name:\n            </th>\n            <th class=\"letter-head-quo\"></th>\n            <th class=\"letter-head-quo\" colspan=\"3\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 10px; border-bottom:1px solid black;\">\n                {{ address.address_line1 }}\n            </th>\n        </tr>\n        <tr class=\"letter-head-quo\">\n            <th class=\"letter-head-quo\" colspan=\"4\" style=\"vertical-align : middle !important;text-align:left !important; border-right:1px solid black; border-bottom:1px solid black;\">\n                {{ doc.customer_name }}\n            </th>\n            <th class=\"letter-head-quo\"></th>\n            <th class=\"letter-head-quo\" colspan=\"2\" rowspan=\"5\" style=\"font-size: 11px !important; vertical-align:top !important;text-align:center; border-right:1px solid black; font-size: 12px !important; border-bottom:1px solid black;\">\n                {{ doc.name }}\n            </th>\n            <th class=\"letter-head-quo\"></th>\n            <th class=\"letter-head-quo\" colspan=\"4\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 10px; border-bottom:1px solid black;\">\n                {{ address.address_line2 }}\n            </th>\n        </tr>\n        <tr class=\"letter-head-quo\" style=\"line-height: 7px !important;\">\n            <!-- <th class=\"letter-head-quo\" colspan=\"2\" rowspan=\"6\"></th> -->\n            <th class=\"letter-head-quo\" colspan=\"4\" rowspan=\"2\" style=\"line-height: 13px !important; vertical-align : middle !important;text-align:left; border:none !important;\">\n                <di>\n                {{ addr_cust }}\n            </di>\n            </th>\n            <th class=\"letter-head-quo\"></th>\n            <th class=\"letter-head-quo\"></th>\n            <th class=\"letter-head-quo\" colspan=\"4\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 10px; border-bottom:1px solid black;\">\n                {{ phone_no}}\n            </th>\n        </tr>\n        <tr class=\"letter-head-quo\">\n            <!-- <th class=\"letter-head-quo\" colspan=\"4\" style=\"vertical-align : middle !important;text-align:justify; border-right:1px solid black; border-bottom:1px solid black;\">\n                \n            </th> -->\n            <th class=\"letter-head-quo\"></th>\n            <th class=\"letter-head-quo\"></th>\n            <th class=\"letter-head-quo\" colspan=\"5\" style=\"line-height: 8px !important; vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 10px; border-bottom:1px solid black;\">\n                AM : {{ fullname }}\n            </th>\n        </tr>\n        <tr class=\"letter-head-quo\">\n            <th class=\"letter-head-quo\" colspan=\"4\" style=\"vertical-align : middle !important;text-align:left !important; border-right:1px solid black; border-bottom:1px solid black;\"> \n                {% if doc.contact_display %}\n                    UP: {{customer_salutation}} {{ doc.contact_display }}\n                {% else %}\n                    UP: No PIC name\n                {% endif %}\n            </th>\n            <th class=\"letter-head-quo\"></th>\n            <th class=\"letter-head-quo\"></th>\n            <th class=\"letter-head-quo\" colspan=\"5\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 10px; border-bottom:1px solid black;\">\n                Phone : {{ phonenum }}\n            </th>\n        </tr>\n        <tr class=\"letter-head-quo\">\n            <th class=\"letter-head-quo\" colspan=\"4\" style=\"vertical-align : middtoptople !important;text-align:left; border-right:1px solid black; border-bottom:1px solid black;\">\n                {% if doc.contact_mobile %}\n                    {{ doc.contact_mobile }}\n                {% else %}\n                    No mobile number\n                {% endif %}\n            </th>\n            <th class=\"letter-head-quo\"></th>\n            <th class=\"letter-head-quo\"></th>\n            <th class=\"letter-head-quo\" colspan=\"5\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 10px; border-bottom:1px solid black;\">\n                Email : {{ email_user }}\n            </th>\n            <!-- <th class=\"letter-head-quo\" style=\"vertical-align : middtoptople !important;text-align:justify; border-right:1px solid black; border-bottom:1px solid black;\">\n                {{ doc.contact_mobile }}\n            </th> -->\n        </tr>\n        <!-- <tr>\n            <th class=\"letter-head-quo\" colspan=\"6\"></th>\n            <th class=\"letter-head-quo\" colspan=\"5\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 10px; border-bottom:1px solid black;\">\n                Email : {{ email_user }}\n            </th>\n            <th class=\"letter-head-quo\" style=\"vertical-align : middtoptople !important;text-align:justify; border-right:1px solid black; border-bottom:1px solid black;\">\n                {{ doc.contact_mobile }}\n            </th>\n        </tr> -->\n        <tr colspan=\"10\" style=\"height: 10px !important;\">\n            <br>\n        </tr>\n    </thead>\n    <tbody>\n        <tr>\n            <td class=\"header-quo\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 11px; border-bottom:1px solid black;\">\n                NO.\n            </td>\n            <td class=\"header-quo\" colspan=\"3\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 11px; border-bottom:1px solid black;\">\n                LOCATION / ROUTE\n            </td>\n            <td class=\"header-quo\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 11px; border-bottom:1px solid black;\">\n                MEDIA TYPE\n            </td>\n            <td class=\"header-quo\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 11px; border-bottom:1px solid black;\">\n                TOTAL SPOT / DAY\n            </td>\n            <td class=\"header-quo\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 11px; border-bottom:1px solid black;\">\n                DURATION\n            </td>\n            <td class=\"header-quo\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 11px; border-bottom:1px solid black;\">\n                SIZE / SIDE\n            </td>\n            <td class=\"header-quo\" colspan=\"2\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 11px; border-bottom:1px solid black;\">\n                PERIOD\n            </td>\n            <td class=\"header-quo\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 11px; border-bottom:1px solid black;\">\n                RATE CARD\n            </td>\n        </tr>\n        {% for prod in doc.items %}\n        <tr style=\"line-height: 7px; \">\n            <td rowspan=\"2\" style=\"vertical-align : middle !important;text-align:center !important;\">{{ loop.index }}</td>\n            <td colspan=\"3\" style=\"vertical-align : middle !important;text-align:left !important;font-weight: bold !important;\">{{ prod.item_name }}</td>\n            <td rowspan=\"2\" style=\"vertical-align : middle !important;text-align:center !important;\">{{ prod.item_type }}</td>\n            <td rowspan=\"2\" style=\"vertical-align : middle !important;text-align:center !important;\">{{ prod.item_spot }}</td>\n            <td rowspan=\"2\" style=\"vertical-align : middle !important;text-align:center !important;\">{{ prod.item_duration }}</td>\n            <td rowspan=\"2\" style=\"vertical-align : middle !important;text-align:center !important;\">{{ prod.dimension }}</td>\n            <td rowspan=\"2\" style=\"vertical-align : middle !important;text-align:center !important;\">\n                {% if prod.period != None %}\n                {{ prod.period.split(' ')[0] | upper }}\n                {% endif %}\n            </td>\n            <td rowspan=\"2\" style=\"vertical-align : middle !important;text-align:center !important;\">\n                {% if prod.period != None %}\n                    {% if prod.period.split(' ')[1] == \"hari\" or prod.period.split(' ')[1] == \"Hari\" %}\n                        DAY\n                    {% elif prod.period.split(' ')[1] == \"minggu\" or prod.period.split(' ')[1] == \"Minggu\" %}\n                        WEEK\n                    {% elif prod.period.split(' ')[1] == \"bulan\" or prod.period.split(' ')[1] == \"Bulan\" %}\n                        MONTH\n                    {% elif prod.period.split(' ')[1] == \"tahun\" or prod.period.split(' ')[1] == \"Tahun\" %}\n                        YEAR\n                    {% endif %}\n                {% endif %}\n            </td>\n            <td rowspan=\"2\" style=\"vertical-align : middle !important;text-align:right !important;\">{{ frappe.format_value(prod.base_rate, {'fieldtype': 'Currency', 'options': 'IDR'}, doc) }}</td>\n        </tr>\n        <tr style=\"line-height: 7px; border-bottom:1px solid black !important;\">\n            <td colspan=\"3\" style=\"line-height: 9px; vertical-align : middle !important;text-align:left !important;\">\n                {% if prod.item_name != \"Marcom Activity\" %}\n                    {{ prod.item_location }}\n                {% elif prod.item_name == \"Marcom Activity\" %}\n                        Price include :\n                        {% if prod.talent != 0 %} <br>\n                            <a style=\"margin-left:10px\">Talent</a>\n                        {% endif %}\n                        {% if prod.wardrobe != 0 %} <br>\n                            <a style=\"margin-left:10px\">Wardrobe</a>\n                        {% endif %}\n                        {% if prod.makeup != 0 %} <br>\n                            <a style=\"margin-left:10px\">Makeup</a>\n                        {% endif %}\n                        {% if prod.perizinan != 0 %} <br>\n                            <a style=\"margin-left:10px\">Permissions (Perizinan)</a>\n                        {% endif %}\n                        {% if prod.spanduk != 0 %} <br>\n                            <a style=\"margin-left:10px\">Banner (Spanduk)</a>\n                        {% endif %}\n                        {% if prod.filler != 0 %} <br>\n                            <a style=\"margin-left:10px\">Documentation Fiiler (Filler Dokumentasi)</a>\n                        {% endif %}\n                        {% if prod.kol != 0 %} <br>\n                            <a style=\"margin-left:10px\">KOL</a>\n                        {% endif %}\n                        {% if prod.paid_ig_buzz != 0 %} <br>\n                            <a style=\"margin-left:10px\">Paid Promote Buzzer on IG</a>\n                        {% endif %}\n                        {% if prod.paid_ig_ming != 0 %} <br>\n                            <a style=\"margin-left:10px\">Paid Promote on IG MING</a>\n                        {% endif %}\n                {% endif %}\n            </td>\n        </tr>\n        {% endfor %}\n        <tr>\n            <td colspan=\"11\" style=\"background-color:#C0C0C0;\"></td>\n        </tr>\n        <tr>\n            <td rowspan=\"6\" class=\"footer-quo\" style=\"border-left:1px solid black !important; border-bottom:1px solid black !important;\"></td>\n            <td colspan=\"5\" class=\"footer-quo\" style=\"text-align:left !important;\">TERMS & CONDITIONS :</td>\n            <td class=\"footer-quo\"></td>\n            <td class=\"footer-quo\" style=\"border-left:1px solid black !important;\"> TOTAL GROSS :</td>\n            <td class=\"footer-quo\" colspan=\"2\" style=\"border-right:1px solid black !important; border-left:1px solid black !important; border-bottom:1px solid black !important;\"></td>\n            <td class=\"footer-quo\" style=\"color: black !important; border-bottom:1px solid black !important; border-left:1px solid black !important; border-right:1px solid black !important;\">{{ frappe.format_value(doc.total, {'fieldtype': 'Currency', 'options': 'IDR'}, doc) }}</td>\n        </tr>\n        <tr>\n            <td class=\"footer-quo\" colspan=\"5\" rowspan=\"5\" style=\"border-bottom:1px solid black !important; text-align: justify !important;\">\n                {% if doc.terms %}\n                    <div style=\"line-height: 3px !important; overflow: hidden !important;\">\n                        {{ TOC }}\n                    </div> \n                {% else %}\n                    Terms and Condition is not created yet\n                {% endif %}\n            </td>\n            <td class=\"footer-quo\"></td>\n            <td class=\"footer-quo\" style=\"border-left:1px solid black !important;\">CASH DISCOUNT :</td>\n            <td colspan=\"2\" class=\"footer-quo\" style=\"border-bottom:1px solid black !important; border-left:1px solid black !important; border-right:1px solid black !important;\"> {{ percent_disc }}%</td>\n            <td class=\"footer-quo\" style=\"color: black !important; border-bottom:1px solid black !important; border-right:1px solid black !important;\">{{ frappe.format_value(doc.discount_amount, {'fieldtype': 'Currency', 'options': 'IDR'}, doc) }}</td>\n        </tr>\n        <tr>\n            <td class=\"footer-quo\"></td>\n            <td class=\"footer-quo\" style=\"border-left:1px solid black !important;\">TOTAL NET :</td>\n            <td class=\"footer-quo\" colspan=\"2\" style=\"border-right:1px solid black !important; border-left:1px solid black !important; border-bottom:1px solid black !important;\"></td>\n            <td class=\"footer-quo\" style=\"color: black !important; bor