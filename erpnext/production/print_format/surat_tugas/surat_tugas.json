pan=\"1\" style=\"width: 100px;\">\n        <col span=\"1\" style=\"width: 20px;\">\n        <col span=\"1\" style=\"width: 100px;\">\n        <col span=\"1\" style=\"width: 100px;\">\n        <col span=\"1\" style=\"width: 20px;\">\n        <col span=\"1\" style=\"width: 80px;\">\n        <col span=\"1\" style=\"width: 80px;\">\n     </colgroup>\n    <thead style=\"border:1px solid black;\">\n        <tr class=\"border-col\">\n            <!-- <th rowspan=\"2\" style=\"vertical-align : middle;text-align:center; border-right:1px solid black;\">No.</th> -->\n            <th rowspan=\"2\" colspan=\"3\" style=\"vertical-align : middle !important; text-align:center !important; border-right:1px solid black; width:200px;\">\n                <img src=\"/files/ming.png\" alt=\"\" height=40px width=130px>\n            </th>\n            <th rowspan=\"2\" colspan=\"3\" style=\"vertical-align : middle;text-align:center; border-right:1px solid black; width:50px;\">\n                <h6>\n                    <b>DATA SURVEY</b>\n                </h6>\n            </th>\n            <th colspan=\"2\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 11px; border-bottom:1px solid black;\">NO DOKUMEN</th>\n            <th style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 11px; border-bottom:1px solid black;\">FR.PROD.008</th>\n            <th rowspan=\"2\" style=\"vertical-align : middle !important; text-align:center !important; border-right:1px solid black;\">\n                <img src=\"/files/ACS.png\" alt=\"\" height=40px width=130px>\n            </th>\n        </tr>\n        <tr class=\"border-col\" style=\"border-top:none !important;\">\n            <th colspan=\"2\" style=\"vertical-align : middle !important;text-align:left; border-right:1px solid black; font-size: 11px;\">NO REVISI</th>\n            <th style=\"vertical-align : middle !important;text-align:left; border-righ